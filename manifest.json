{
  "name": "Copy ValueBiz.com",
  "version": "1.0",
  "description": "As soon as you browse http://valuebiz.com, it'll copy the content to your clipboard!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://www.valuebiz.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "page_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ]
}